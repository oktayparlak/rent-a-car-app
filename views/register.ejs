    <div class="container mt-5">
        <div class="row">
          <div class="col-md-6 offset-md-3">
            <h1 class="text-center mb-5">Üye Ol</h1>
            <form>
                <div class="form-group">
                    <label for="firstname">Adınız</label>
                    <input type="text" class="form-control" id="firstname" placeholder="Adınızı girin">
                  </div>
                  <div class="form-group">
                    <label for="lastname">Soyadınız</label>
                    <input type="text" class="form-control" id="lastname" placeholder="Soyadınızı girin">
                  </div>
                  <div class="form-group">
                    <label for="email">Email adresiniz</label>
                    <input type="email" class="form-control" id="email" placeholder="Email adresinizi girin">
                  </div>
                  <div class="form-group">
                    <label for="password">Şifreniz</label>
                    <input type="password" class="form-control" id="password" placeholder="Şifrenizi girin">
                  </div>
                  <div class="form-group">
                  <input type="checkbox" class="form-check-input" id="checkbox">
                  <label class="form-check-label" for="exampleCheck1">Üyelik Sözlemeşini Kabul Ediyorum.</label>
                  </div>
                  <button type="submit" class="btn btn-primary btn-block">Üye Ol</button>
            </form>
            <br><br>

          </div>
        </div>
      </div>

